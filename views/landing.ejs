        function initEstoqueDemo() {
            const grid = document.getElementById('estoque-grid');
            if (!grid || grid.dataset.init) return;
            grid.dataset.init = 'true';
           
            function render(lista) {
                grid.innerHTML = lista.length === 0
                    ? `<div style="grid-column:1/-1;text-align:center;padding:4rem;color:#95a5a6;"><i class="fas fa-search" style="font-size:4rem;margin-bottom:1.5rem;"></i><p style="font-size:1.2rem;">Nenhum item encontrado</p></div>`
                    : lista.map(p => `
                        <div style="background:white;border-radius:16px;border:2px solid ${p.status==='Crítico'?'#e74c3c':p.status==='Vencendo'?'#f39c12':'#e9ecef'};overflow:hidden;box-shadow:0 10px 30px rgba(0,0,0,0.1);transition:transform 0.3s ease;">
                            <div style="padding:1.5rem;background:${p.status==='Crítico'?'#fdf2f2':p.status==='Vencendo'?'#fff5e7':'#f8f9fa'};display:flex;justify-content:space-between;align-items:center;">
                                <div><strong style="font-size:1.3rem;">${p.nome}</strong><br><small style="color:#7f8c8d;font-size:0.9rem;">#${p.id} • ${p.loc}</small></div>
                                <span style="background:#d4edda;color:#155724;padding:0.5rem 1.2rem;border-radius:50px;font-size:0.9rem;font-weight:600;">${p.cat}</span>
                            </div>
                            <div style="padding:1.5rem;">
                                <div style="display:flex;justify-content:space-between;margin:1.5rem 0;">
                                    <div><small style="color:#95a5a6;font-size:0.9rem;">Quantidade</small>
                                        <div style="font-size:2rem;font-weight:800;color:${p.status==='Crítico'||p.status==='Baixo'?'#e74c3c':'#27ae60'}">${p.qtd} ${p.un}</div>
                                    </div>
                                    <div style="text-align:right;"><small style="color:#95a5a6;font-size:0.9rem;">Validade</small>
                                        <div style="font-weight:700;font-size:1.2rem;color:${p.status==='Vencendo'?'#e67e22':'#2c3e50'}">${p.val}</div>
                                    </div>
                                </div>
                                <div style="display:flex;gap:0.75rem;flex-wrap:wrap;">
                                    <span style="background:${p.status==='Disponível'?'#d1ecf1':p.status==='Baixo'?'#fff3cd':p.status==='Crítico'?'#f8d7da':'#fff5e7'};
                                                color:${p.status==='Disponível'?'#0c5460':p.status==='Baixo'?'#856404':p.status==='Crítico'?'#721c24':'#e67e22'};
                                                padding:0.6rem 1.2rem;border-radius:50px;font-size:0.9rem;font-weight:600;">
                                        ${p.status==='Crítico'?'Estoque crítico':p.status==='Vencendo'?'Vencendo em breve':p.status}
                                    </span>
                                    <span style="background:#f8d7da;color:#721c24;padding:0.6rem 1.2rem;border-radius:50px;font-size:0.9rem;font-weight:600;">${p.risco} risco</span>
                                </div>
                            </div>
                        </div>
                    `).join('');
            }
           
            function filtrar() {
                const busca = (document.getElementById('estoque-search')?.value || '').toLowerCase();
                const cat = document.getElementById('estoque-categoria')?.value || '';
                const status = document.getElementById('estoque-status')?.value || '';
                const filtrados = produtosDemo.filter(p => {
                    const matchBusca = p.nome.toLowerCase().includes(busca) || p.id.includes(busca) || p.loc.toLowerCase().includes(busca);
                    const matchCat = !cat || p.cat === cat;
                    const matchStatus = !status || p.status === status;
                    return matchBusca && matchCat && matchStatus;
                });
                render(filtrados);
            }
           
            render(produtosDemo);
            document.getElementById('estoque-search')?.addEventListener('input', filtrar);
            document.getElementById('estoque-categoria')?.addEventListener('change', filtrar);
            document.getElementById('estoque-status')?.addEventListener('change', filtrar);
        }
       
