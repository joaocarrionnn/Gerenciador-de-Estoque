        // Função para abrir o conteúdo expandido
        function openExpandedContent(exampleKey) {
            const content = examplesContent[exampleKey];
            if (!content) return;
           
            // Atualiza o título e descrição
            expandedTitle.textContent = content.title;
            expandedDescription.textContent = content.description;
           
            // Insere o conteúdo
            expandedContent.innerHTML = content.content;
           
            // Mostra o overlay e o container expandido
            overlay.classList.add('active');
            expandedContainer.classList.add('active');
           
            // Remove scroll do body
            document.body.style.overflow = 'hidden';
           
            // Ativa os componentes interativos se necessário
            setTimeout(() => {
                if (exampleKey === 'estoque') {
                    initEstoqueDemo();
                } else if (exampleKey === 'relatorios') {
                    initRelatoriosDemo();
                }
            }, 100);
        }
       
        // Adiciona eventos de clique aos cards
        featureCards.forEach(card => {
            card.addEventListener('click', () => {
                const exampleKey = card.dataset.example;
                openExpandedContent(exampleKey);
            });
        });
       
        // Funções para fechar
        function closeExpandedContent() {
            overlay.classList.remove('active');
            expandedContainer.classList.remove('active');
            document.body.style.overflow = '';
           
            // Remove o estado ativo dos cards
            featureCards.forEach(card => card.classList.remove('active'));
        }
       
        // Botão de voltar
        document.querySelector('.feature-back-button').addEventListener('click', closeExpandedContent);
       
        // Botão de fechar (X)
        document.querySelector('.close-expanded').addEventListener('click', closeExpandedContent);
       
        // Fecha ao clicar no overlay
        overlay.addEventListener('click', closeExpandedContent);
       
        // Fecha com ESC
        document.addEventListener('keydown', (e) => {
            if (e.key === 'Escape' && expandedContainer.classList.contains('active')) {
                closeExpandedContent();
            }
        });

